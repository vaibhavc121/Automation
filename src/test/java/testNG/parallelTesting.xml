<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="5" name="Single/Sequential/Parallel execution" parallel="tests">
	
  <test name="edgeTest">
	<parameter name="browser" value="edge"></parameter>
	<parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"></parameter>
    <classes>
      <class name="testNG.ParallelTesting"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="chromeTest">
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"></parameter>
    <classes>
      <class name="testNG.ParallelTesting"/>
    </classes>
  </test> <!-- Test -->  
  
</suite> <!-- Suite -->
